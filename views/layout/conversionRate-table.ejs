<section class="currency-comparison-table content">
    <div class="container">
        <div class="wrap-content">
            <div style="padding: 0 20px">
                <div class="title-currency-compare text-center">
                    <span class="">Exchange rates</span>
                </div>
                <table class="table">
                    <tbody id="body-table-compare" class="body-table">
                    <tr id="head-table">
                        <th scope="col" class="col-table-title">
                            <img src="../images/line/first-line.a04110167e93cd1ba56f7b6c590d23e0.png">
                            <label class="label-btn" style="">
                                <style>
                                    .toggle.ios, .toggle-on.ios, .toggle-off.ios { border-radius: 20rem; }
                                    .toggle.ios .toggle-handle { border-radius: 20rem; }
                                </style>
                                <div class="text-reverst" style="font-size: 1.3em; color: #002272;padding-right: 15px">Reverse</div>
                                <input id="toggle-event" type="checkbox" checked data-toggle="toggle" data-style="ios" data-on=" " data-off=" ">
                            </label>
                        </th>
                        <th id="<%=data[unitFrom].KiHieu %>" scope="col" class="col-table-1">
                            <div class="wrap-img">
                                <img src="<%=data[unitFrom].QuocKi %>">
                            </div>
                            <div class="title-currency text-center"><a href="/Currency/Convert?from=<%=data[unitFrom].KiHieu %>"><%=data[unitFrom].KiHieu%></a></div>
                        </th>
                        <th id="<%=data[unitTo].KiHieu %>" scope="col" class="col-table-1">
                            <div class="wrap-img">
                                <img src="<%=data[unitTo].QuocKi %>">
                            </div>
                            <div class="title-currency text-center"><a href="/Currency/Convert?from=<%=data[unitTo].KiHieu %>"><%=data[unitTo].KiHieu %></a></div>
                        </th>
                        <th id="<%=data[37].KiHieu %>" scope="col" class="col-table-1">
                            <div class="wrap-img">
                                <img src="<%=data[37].QuocKi %>">
                            </div>
                            <div class="title-currency text-center"><a href="/Currency/Convert?from=<%=data[37].KiHieu %>"><%=data[37].KiHieu %></a></div>
                        </th>
                        <th id="<%=data[15].KiHieu %>" scope="col" class="col-table-1">
                            <div class="wrap-img">
                                <img src="<%=data[15].QuocKi %>">
                            </div>
                            <div class="title-currency text-center"><a href="/Currency/Convert?from=<%=data[15].KiHieu%>"><%=data[15].KiHieu %></a></div>
                        </th>
                        <th id="<%=data[31].KiHieu %>" scope="col" class="col-table-1">
                            <div class="wrap-img">
                                <img src="<%=data[31].QuocKi %>">
                            </div>
                            <div class="title-currency text-center"><a href="/Currency/Convert?from=<%=data[31].KiHieu%>"><%=data[31].KiHieu %></a></div>
                        </th>
                        <th id="<%=data[27].KiHieu %>" scope="col" class="col-table-1">
                            <div class="wrap-img">
                                <img src="<%=data[27].QuocKi %>">
                            </div>
                            <div class="title-currency text-center"><a href="/Currency/Convert?from=<%=data[27].KiHieu %>"><%=data[27].KiHieu %></a></div>
                        </th>
                    </tr>
                    <tr class="unit-table">
                        <th id="<%=data[unitFrom].KiHieu%>" scope="col" class="col-table-title">
                            <img src="../images/line/middle-line.78c3ca4692f73d84aa1cc55a36fc7aef.png">
                            <div class="wrap-img-row">
                                <img src="<%=data[unitFrom].QuocKi %>">
                            </div>
                            <div class="title-currency text-center title-currency-row"><a href="/Currency/Convert?from=<%=data[unitFrom].KiHieu%>" >1 <%=data[unitFrom].KiHieu%></a></div>
                        </th>
                        <th scope="col" class="col-table-1 font-size-number text-center padding-top">
                            <span><%=(data[unitFrom].TiLe/data[unitFrom].TiLe).toPrecision(5)%></span>
                        </th>
                        <th scope="col" class="col-table-1 font-size-number text-center padding-top">
                            <span><%=(data[unitFrom].TiLe/data[unitTo].TiLe).toPrecision(5)%></span>
                        </th>
                        <th scope="col" class="col-table-1 font-size-number text-center padding-top">
                            <span><%=(data[unitFrom].TiLe/data[37].TiLe).toPrecision(5)%></span>
                        </th>
                        <th scope="col" class="col-table-1 font-size-number text-center padding-top">
                            <span><%=(data[unitFrom].TiLe/data[15].TiLe).toPrecision(5)%></span>
                        </th>
                        <th scope="col" class="col-table-1 font-size-number text-center padding-top">
                            <span><%=(data[unitFrom].TiLe/data[31].TiLe).toPrecision(5)%></span>
                        </th>
                        <th scope="col" class="col-table-1 font-size-number text-center padding-top">
                            <span><%=(data[unitFrom].TiLe/data[27].TiLe).toPrecision(5)%></span>
                        </th>
                    </tr>
                    <tr class="display-none unit-table"></tr>
                    <tr class="display-none unit-table"></tr>
                    <tr class="display-none unit-table"></tr>
                    <tr class="display-none unit-table"></tr>
                    <tr class="display-none unit-table"></tr>
                    <tr>
                        <th scope="col" class="col-table-title">
                            <img src="../images/line/first-line.a04110167e93cd1ba56f7b6c590d23e0.png">
                        </th>
                    </tr>
                    </tbody>
                </table>
            </div>
            <div class="footer-compare-currency">
                <div class="wrap-btn-add-remove" style="width: 20%;position: relative">
                    <a href="javascrip:void(0)">
                        <div id="btn-add-remove" class="set-btn-add-remove" >
                            <div class="btn-add"><svg aria-hidden="true" data-id="icon-plus-circle" viewBox="0 0 40 40" height="40px" style="min-width: 40px;"><g fill="none" fill-rule="evenodd"><circle cx="20" cy="20" r="19.5" stroke="currentColor"></circle><rect width="2" height="18" x="19" y="11" fill="currentColor" rx="1"></rect><rect width="18" height="2" x="11" y="19" fill="currentColor" rx="1"></rect></g></svg></div>
                            <div class="text-center text-btn">Add/Remove Coin</div>
                        </div>
                    </a>
                    <input id="input-add-remove" class="form-control set-from-input display-none">
                    <div class="dropdown">
                        <ul id="dropdown-add-remove" class="dropdown-menu">
                            <%data.map((e) => {%>
                            <li id="<%=e.KiHieu%>" class="dropdown-item">
                                <a href="javascript:void(0)" class="dropdown-item-1">
                                    <div class="name-option-1" style="top:5%; left: 6%">
                                        <div class="text-name-option" >
                                            <div class="current-code"><%=e.KiHieu%> </div> - <div class="current-name"> <%=e.TenDonVi%></div><div class="icon-remove display-none"><i class="fas fa-minus-circle" style="width: 40px!important;height: 40px!important;color: red"></i></div>
                                        </div>
                                    </div>
                                </a>
                            </li>
                            <%})%>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<script type="text/javascript" src="../javascripts/conversion-currency-ajax.js"></script>
<script type="text/javascript" src="../javascripts/add-remove-currency.js"></script>
